{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "project_code",
	"projects": {
		"app1": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "project_code/app1",
			"sourceRoot": "project_code/app1/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/app1",
						"index": "project_code/app1/src/index.html",
						"main": "project_code/app1/src/main.ts",
						"polyfills": "project_code/app1/src/polyfills.ts",
						"tsConfig": "project_code/app1/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"project_code/app1/src/favicon.ico",
							"project_code/app1/src/assets"
						],
						"styles": [
							"project_code/app1/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "project_code/app1/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "project_code/app1/src/environments/environment.ts",
									"with": "project_code/app1/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "project_code/app1/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "app1:build:production",
							"extraWebpackConfig": "project_code/app1/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "app1:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 8001,
						"publicHost": "http://localhost:8001",
						"extraWebpackConfig": "project_code/app1/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "app1:build",
						"extraWebpackConfig": "project_code/app1/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "project_code/app1/src/test.ts",
						"polyfills": "project_code/app1/src/polyfills.ts",
						"tsConfig": "project_code/app1/tsconfig.spec.json",
						"karmaConfig": "project_code/app1/karma.conf.js",
						"inlineStyleLanguage": "scss",
						"assets": [
							"project_code/app1/src/favicon.ico",
							"project_code/app1/src/assets"
						],
						"styles": [
							"project_code/app1/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		},
